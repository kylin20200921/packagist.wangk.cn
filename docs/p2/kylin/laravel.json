{
    "packages": {
        "kylin/laravel": [
            {
                "name": "kylin/laravel",
                "version": "0.0.1",
                "version_normalized": "0.0.1.0",
                "source": {
                    "type": "git",
                    "url": "https://gitee.com/kylin20200921/laravel.wangk.cn.git",
                    "reference": "97ae7ea3cf6ca1f0f4f4829ff95a8c7d81df4405"
                },
                "dist": {
                    "type": "tar",
                    "url": "https://packagist.wangk.cn/dist/kylin/laravel/kylin-laravel-0.0.1-040505.tar",
                    "reference": "97ae7ea3cf6ca1f0f4f4829ff95a8c7d81df4405",
                    "shasum": "c8f61ad6be7d7eabc4571563299c8804571a63a6"
                },
                "require": {
                    "akaunting/laravel-firewall": "^2.2",
                    "hisorange/browser-detect": "^4.5",
                    "intervention/image": "^3.9",
                    "kylin/admin": "^0.0.1",
                    "kylin/modules": "^0.0.1",
                    "laravel/framework": "^11.31",
                    "laravel/sanctum": "^4.0",
                    "laravel/socialite": "^5.16",
                    "laravel/tinker": "^2.9",
                    "php": "^8.3",
                    "spatie/laravel-permission": "^6.10"
                },
                "require-dev": {
                    "barryvdh/laravel-ide-helper": "^3.2",
                    "fakerphp/faker": "^1.23",
                    "laravel/pail": "^1.1",
                    "laravel/pint": "^1.13",
                    "laravel/sail": "^1.26",
                    "mockery/mockery": "^1.6",
                    "nunomaduro/collision": "^8.1",
                    "phpunit/phpunit": "^11.0.1"
                },
                "time": "2024-11-22T14:07:12+00:00",
                "type": "project",
                "extra": {
                    "laravel": {
                        "dont-discover": []
                    },
                    "merge-plugin": {
                        "include": [
                            "packages/*/composer.json"
                        ]
                    }
                },
                "installation-source": "source",
                "autoload": {
                    "psr-4": {
                        "App\\": "app/",
                        "Database\\Factories\\": "database/factories/",
                        "Database\\Seeders\\": "database/seeders/"
                    },
                    "files": [
                        "bootstrap/helpers.php"
                    ]
                },
                "autoload-dev": {
                    "psr-4": {
                        "Tests\\": "tests/"
                    }
                },
                "scripts": {
                    "post-autoload-dump": [
                        "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
                        "@php artisan package:discover --ansi"
                    ],
                    "post-update-cmd": [
                        "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
                    ],
                    "post-root-package-install": [
                        "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
                    ],
                    "post-create-project-cmd": [
                        "@php artisan key:generate --ansi",
                        "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
                        "@php artisan vendor:publish --provider=\"Kylin\\Modules\\LaravelModulesServiceProvider\" --tag=\"config\"",
                        "@php artisan vendor:publish --provider=\"Laravel\\Sanctum\\SanctumServiceProvider\"",
                        "@php artisan vendor:publish --tag=\"firewall\"",
                        "@php artisan telescope:install",
                        "@php artisan admin:publish",
                        "@php artisan migrate --graceful --ansi",
                        "@php artisan admin:install"
                    ],
                    "dev": [
                        "Composer\\Config::disableProcessTimeout",
                        "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
                    ]
                },
                "license": [
                    "MIT"
                ],
                "description": "麒麟集团",
                "keywords": [
                    "laravel",
                    "麒麟集团"
                ]
            }
        ]
    }
}
