{
    "packages": {
        "kylin/kylin": [
            {
                "name": "kylin/kylin",
                "version": "dev-main",
                "version_normalized": "dev-main",
                "source": {
                    "type": "git",
                    "url": "https://gitee.com/kylin20200921/project.wangk.cn.git",
                    "reference": "861ac7a4db14389dba8ba52991e619fb7d9b9132"
                },
                "require": {
                    "akaunting/laravel-firewall": "^2.2",
                    "hisorange/browser-detect": "^4.5",
                    "intervention/image": "^3.7",
                    "kylin/admin": "^0.0.1",
                    "kylin/modules": "^0.0.1",
                    "laravel/framework": "^11.9",
                    "laravel/octane": "^2.5",
                    "laravel/sanctum": "^4.0",
                    "laravel/socialite": "^5.15",
                    "laravel/tinker": "^2.9",
                    "php": "^8.2",
                    "spatie/laravel-permission": "^6.9"
                },
                "require-dev": {
                    "barryvdh/laravel-ide-helper": "^3.1",
                    "fakerphp/faker": "^1.23",
                    "laravel/pint": "^1.13",
                    "laravel/sail": "^1.26",
                    "laravel/telescope": "^5.2",
                    "mockery/mockery": "^1.6",
                    "nunomaduro/collision": "^8.0",
                    "phpunit/phpunit": "^11.0.1",
                    "spatie/laravel-ignition": "^2.8"
                },
                "time": "2024-08-12T09:34:04+00:00",
                "default-branch": true,
                "type": "project",
                "extra": {
                    "laravel": {
                        "dont-discover": []
                    },
                    "merge-plugin": {
                        "include": [
                            "packages/*/composer.json"
                        ]
                    }
                },
                "autoload": {
                    "psr-4": {
                        "App\\": "app/",
                        "Database\\Factories\\": "database/factories/",
                        "Database\\Seeders\\": "database/seeders/"
                    },
                    "files": [
                        "bootstrap/helpers.php"
                    ]
                },
                "autoload-dev": {
                    "psr-4": {
                        "Tests\\": "tests/"
                    }
                },
                "scripts": {
                    "post-autoload-dump": [
                        "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
                        "@php artisan package:discover --ansi"
                    ],
                    "post-update-cmd": [
                        "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
                    ],
                    "post-root-package-install": [
                        "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
                    ],
                    "post-create-project-cmd": [
                        "@php artisan key:generate --ansi",
                        "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
                        "@php artisan vendor:publish --provider=\"Kylin\\Modules\\LaravelModulesServiceProvider\" --tag=\"config\"",
                        "@php artisan vendor:publish --provider=\"Laravel\\Sanctum\\SanctumServiceProvider\"",
                        "@php artisan vendor:publish --tag=\"firewall\"",
                        "@php artisan admin:publish",
                        "@php artisan migrate --graceful --ansi",
                        "@php artisan admin:install"
                    ]
                },
                "license": [
                    "MIT"
                ],
                "description": "麒麟集团",
                "keywords": [
                    "Kylin",
                    "系统模板"
                ]
            }
        ]
    }
}
