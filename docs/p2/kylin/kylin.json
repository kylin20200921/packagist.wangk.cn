{
    "packages": {
        "kylin/kylin": [
            {
                "name": "kylin/kylin",
                "version": "1.0.0.0",
                "version_normalized": "1.0.0.0",
                "source": {
                    "type": "git",
                    "url": "https://gitee.com/kylin20200921/project.wangk.cn.git",
                    "reference": "96caeffba5c5c72e7e6cf8040c6c7eda1e6152c6"
                },
                "dist": {
                    "type": "tar",
                    "url": "https://packagist.wangk.cn/dist/kylin/kylin/kylin-kylin-1.0.0.0-f0bcee.tar",
                    "reference": "96caeffba5c5c72e7e6cf8040c6c7eda1e6152c6",
                    "shasum": "a146aebd42e3365af3cb8e1647abb18e3599366a"
                },
                "require": {
                    "akaunting/laravel-firewall": "*",
                    "kylin/modules": "^1.0",
                    "laravel/framework": "^11.0",
                    "laravel/sanctum": "^4.0",
                    "laravel/tinker": "^2.9",
                    "php": "^8.2"
                },
                "require-dev": {
                    "fakerphp/faker": "^1.23",
                    "laravel/pint": "^1.13",
                    "laravel/sail": "^1.26",
                    "mockery/mockery": "^1.6",
                    "nunomaduro/collision": "^8.0",
                    "phpunit/phpunit": "^10.5",
                    "spatie/laravel-ignition": "^2.4"
                },
                "time": "2024-03-26T17:29:22+00:00",
                "type": "project",
                "extra": {
                    "laravel": {
                        "dont-discover": []
                    },
                    "merge-plugin": {
                        "include": [
                            "packages/*/composer.json"
                        ]
                    }
                },
                "installation-source": "source",
                "autoload": {
                    "psr-4": {
                        "App\\": "app/",
                        "Database\\Factories\\": "database/factories/",
                        "Database\\Seeders\\": "database/seeders/"
                    },
                    "files": [
                        "app/helpers.php"
                    ]
                },
                "autoload-dev": {
                    "psr-4": {
                        "Tests\\": "tests/"
                    }
                },
                "scripts": {
                    "post-autoload-dump": [
                        "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
                        "@php artisan package:discover --ansi"
                    ],
                    "post-update-cmd": [
                        "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
                    ],
                    "post-root-package-install": [
                        "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
                    ],
                    "post-create-project-cmd": [
                        "@php artisan key:generate --ansi",
                        "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
                        "@php artisan migrate --graceful --ansi",
                        "@php artisan vendor:publish --provider=\"Kylin\\Modules\\LaravelModulesServiceProvider\" --tag=\"config\"",
                        "@php artisan vendor:publish --tag=\"firewall\""
                    ]
                },
                "license": [
                    "MIT"
                ],
                "description": "麒麟集团",
                "keywords": [
                    "Kylin",
                    "系统模板"
                ]
            }
        ]
    }
}
